{
  "compilerOptions": {
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "Node16",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node16",
    "importHelpers": false,
    "strict": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts"
  ]
}
